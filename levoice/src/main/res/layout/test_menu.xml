<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity" >

    <Button
        android:id="@+id/test_contacts"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="联系人" />

    <Button
        android:id="@+id/test_app"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="应用" />

    <Button
        android:id="@+id/test_vod"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="VOD" />
    <Button
        android:id="@+id/test_was"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="网站" />

    <Button
        android:id="@+id/test_all"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="全部领域" />
    
    <Button
        android:id="@+id/test_auto"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="自动测试TTS" />
    <Button
        android:id="@+id/TestAutoRunPcm"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="Pcm 测试" />
    
      <Button
        android:id="@+id/EditAbleParam"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="参数测试" />
      
       <Button
        android:id="@+id/autotest"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="全功能自动测试" />
    <Button
        android:id="@+id/test_TestBuildGrammar"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="BuildGrammar" />
    
</LinearLayout>